{
  "name": "ewewing_django_mock",
  "description": "mock env for ewewing_django",
  "chef_type": "environment",
  "json_class": "Chef::Environment",
  "default_attributes": {
    "timezone": {
      "name": "Asia/Tokyo"
    },
    "swap": {
      "path": "/swapfile",
      "size": 1024
    },
    "user": {
      "name":"django_mock"
    },
    "apache": {
      "ports": [
        "80",
        "443"
      ],
      "projects": []
    },
    "mysql": {
      "repository": "mysql-community-release-el6-7.noarch.rpm",
      "db_host": "localhost",
      "db_names": [
        "enybank_web",
        "enybank_bot",
        "isekai_shokudo"
      ],
      "config": {
        "charset": "utf8",
        "collate": "utf8_general_ci",
        "general_log": "1",
        "slow_query_log": "1",
        "long_query_time": "5.0",
        "log_queries_not_using_indexes": "0",
        "server_id": "1",
        "log_bin": "bin",
        "hostname_in_log_bin": true,
        "expire_logs_days": "7"
      }
    },
    "git": {
      "version": "2.22.0"
    },
    "miniconda": {
      "version": "4.3.30",
      "python": {
        "version": "3.6"
      }
    },
    "django": {
      "packages": {
        "django": {
          "pkg": "django",
          "dir": "django"
        },
        "mysqlclient": {
          "pkg": "mysqlclient",
          "dir": "MySQLdb"
        },
        "pymysql": {
          "pkg": "pymysql",
          "dir": "pymysql"
        },
        "pillow": {
          "pkg": "Pillow",
          "dir": "PIL"
        },
        "linebot": {
          "pkg": "line-bot-sdk",
          "dir": "linebot"
        },
        "stripe": {
          "pkg": "stripe",
          "dir": "stripe"
        },
        "social_auth_app_django": {
          "pkg": "social-auth-app-django",
          "dir": "social_django"
        },
        "django_storages_google": {
          "pkg": "django-storages[google]",
          "dir": "storages"
        },
        "beautifulsoup4": {
          "pkg": "beautifulsoup4",
          "dir": "bs4"
        }
      }
    },
    "repository": {
      "repositories": [
        {
          "owner":"anybirth",
          "name":"enybank_web"
        },
        {
          "owner":"anybirth",
          "name":"enybank_bot"
        },
        {
          "owner":"tab0902",
          "name":"isekai_shokudo"
        }
      ]
    },
    "certbot": {
      "webroot":"/var/www/html",
      "email":"tabuchi@ewewing.com",
      "domains": {
        "enybank_web": "enybank.ewewing.work",
        "isekai_shokudo": "isekai-shokudo.ewewing.work"
      }
    },
    "mod_wsgi": {
      "ports": [
        "80",
        "443"
      ],
      "projects": [
        {
          "directory":"enybank_web",
          "host":"enybank.ewewing.com",
          "port": {
            "secure": "443",
            "insecure": "80"
          }
        },
        {
          "directory":"isekai_shokudo",
          "host":"isekai-shokudo.ewewing.work",
          "port": {
            "secure": "443",
            "insecure": "80"
          }
        }
      ]
    }
  },
  "override_attributes": {
  },
  "cookbook_versions": {
  }
}
