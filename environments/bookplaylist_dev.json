{
  "name": "bookplaylist_dev",
  "description": "development env for bookplaylist",
  "chef_type": "environment",
  "json_class": "Chef::Environment",
  "default_attributes": {
    "mysql": {
      "config": {
        "log_error": "error.log",
        "general_log": "1",
        "general_log_file": "general.log",
        "slow_query_log": "1",
        "slow_query_log_file": "slow.log",
        "long_query_time": "5.0",
        "log_queries_not_using_indexes": "0",
        "server_id": "1",
        "log_bin": "bin",
        "hostname_in_log_bin": false,
        "expire_logs_days": "7"
      }
    }
  },
  "override_attributes": {
  },
  "cookbook_versions": {
  }
}
