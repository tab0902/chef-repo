{
  "name": "reform_love_stg",
  "description": "staging env for reform_love",
  "chef_type": "environment",
  "json_class": "Chef::Environment",
  "default_attributes": {
    "timezone": {
      "name": "Asia/Tokyo"
    },
    "user": {
      "name":"stg",
      "password":"$1$nF8Jj3gC$yK9qTQTY0Um3Cmnc4/lzF0"
    },
    "apache": {
      "ports": [
        "80",
        "443"
      ],
      "projects": [
        {
          "directory":"reform_love/webroot",
          "host":"stg.j7l4vmr2.reform-love.com",
          "port": {
            "secure": "443",
            "insecure": "80"
          }
        }
      ]
    },
    "mysql": {
      "repository": "mysql-community-release-el6-7.noarch.rpm",
      "db_host": "localhost",
      "db_names": [
        "reform_love",
        "reform_love_wp"
      ],
      "config": {
        "charset": "utf8mb4",
        "collate": "utf8mb4_bin",
        "log_error": "error.log",
        "general_log": "1",
        "general_log_file": "general.log",
        "slow_query_log": "1",
        "slow_query_log_file": "slow.log",
        "long_query_time": "5.0",
        "log_queries_not_using_indexes": "0",
        "server_id": "1",
        "log_bin": "bin",
        "hostname_in_log_bin": false,
        "expire_logs_days": "7"
      }
    },
    "git": {
      "version": "2.22.0"
    },
    "repository": {
      "repositories": [
        {
          "owner":"tab0902",
          "name":"reform_love"
        },
        {
          "owner":"tab0902",
          "name":"reform_love_admin"
        }
      ]
    },
    "php": {
      "version": "73",
      "timezone": "Asia/Tokyo",
      "language": "Japanese",
      "encoding": "UTF-8",
      "packages": [
        "php",
        "php-devel",
        "php-mysqlnd",
        "php-pdo",
        "php-gd",
        "php-mbstring",
        "php-opcache",
        "php-xml",
        "php-pecl-xdebug"
      ]
    },
    "cakephp": {
      "projects": [
        "reform_love"
      ]
    },
    "wordpress": {
      "projects": {
        "reform_love_wp": {
          "destination": "/reform_love/webroot",
          "directory": "column",
          "db_name": "reform_love_wp"
        }
      }
    },
    "miniconda": {
      "version": "4.3.30",
      "python": {
        "version": "3.6"
      }
    },
    "django": {
      "packages": {
        "django": {
          "pkg": "django",
          "dir": "django"
        },
        "mysqlclient": {
          "pkg": "mysqlclient",
          "dir": "MySQLdb"
        },
        "pymysql": {
          "pkg": "pymysql",
          "dir": "pymysql"
        },
        "pillow": {
          "pkg": "Pillow",
          "dir": "PIL"
        },
        "django_modeladmin_reorder": {
          "pkg": "django-modeladmin-reorder",
          "dir": "admin_reorder"
        },
        "beautifulsoup4": {
          "pkg": "beautifulsoup4",
          "dir": "bs4"
        }
      }
    },
    "certbot": {
      "webroot":"/var/www/html",
      "email":"tabuchi@ewewing.com",
      "domains": {
        "reform_love": "stg.j7l4vmr2.reform-love.com",
        "reform_love_admin": "stg.j7l4vmr2.admin.reform-love.com"
      }
    },
    "mod_wsgi": {
      "projects": [
        {
          "directory":"reform_love_admin",
          "host":"stg.j7l4vmr2.admin.reform-love.com",
          "port": {
            "secure": "443",
            "insecure": "80"
          }
        }
      ]
    }
  },
  "override_attributes": {
  },
  "cookbook_versions": {
  }
