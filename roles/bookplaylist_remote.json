{
  "name": "bookplaylist_remote",
  "description": "remote role for bookplaylist",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {
    "timezone": {
      "name": "Asia/Tokyo"
    },
    "mysql": {
      "repository": "mysql-community-release-el7-7.noarch.rpm",
      "db_host": "localhost",
      "db_names": [
        "bookplaylist"
      ],
      "config": {
        "charset": "utf8mb4",
        "collate": "utf8mb4_bin",
        "timezone": "UTC"
      }
    },
    "git": {
      "version": "2.23.0"
    },
    "wkhtmltopdf": {
      "version": "0.12.4"
    },
    "repository": {
      "repositories": [
        {
          "owner":"tab0902",
          "name":"bookplaylist"
        }
      ]
    },
    "miniconda": {
      "version": "4.3.30",
      "python": {
        "version": "3.6"
      }
    },
    "django": {
      "packages": {
        "django": {
          "pkg": "django",
          "dir": "django"
        },
        "mysqlclient": {
          "pkg": "mysqlclient",
          "dir": "MySQLdb"
        },
        "pymysql": {
          "pkg": "pymysql",
          "dir": "pymysql"
        },
        "pillow": {
          "pkg": "Pillow",
          "dir": "PIL"
        },
        "imgkit": {
          "pkg": "imgkit",
          "dir": "imgkit"
        },
        "django_modeladmin_reorder": {
          "pkg": "django-modeladmin-reorder",
          "dir": "admin_reorder"
        },
        "social_auth_app_django": {
          "pkg": "social-auth-app-django",
          "dir": "social_django"
        },
        "django_storages_google": {
          "pkg": "django-storages[google]",
          "dir": "storages"
        }
      },
      "projects": [
        "bookplaylist"
      ]
    },
    "certbot": {
      "webroot":"/var/www/html",
      "email":"admin@booxmix.com"
    }
  },
  "override_attributes": {
  },
  "run_list": [
    "recipe[timezone]",
    "recipe[user]",
    "recipe[yum]",
    "recipe[epel]",
    "recipe[vim]",
    "recipe[apache]",
    "recipe[mysql]",
    "recipe[git]",
    "recipe[wkhtmltopdf]",
    "recipe[repository]",
    "recipe[pyenv]",
    "recipe[miniconda]",
    "recipe[django]",
    "recipe[certbot]",
    "recipe[mod_wsgi]"
  ]
}
