{
  "name": "enybank_web_local",
  "description": "local environment for enybank_web",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {
    "mysql": {
      "repository": "mysql-community-release-el6-7.noarch.rpm",
      "db_names": [
        "enybank_web"
      ]
    },
    "git": {
      "version": "2.18.0"
    },
    "miniconda": {
      "version": "4.3.30",
      "python": {
        "version": "3.6"
      }
    },
    "django": {
      "packages": {
        "django": {
          "pkg": "django",
          "dir": "django"
        },
        "mysqlclient": {
          "pkg": "mysqlclient",
          "dir": "MySQLdb"
        },
        "pymysql": {
          "pkg": "pymysql",
          "dir": "pymysql"
        },
        "pillow": {
          "pkg": "Pillow",
          "dir": "PIL"
        },
        "social_auth_app_django": {
          "pkg": "social-auth-app-django",
          "dir": "social_django"
        },
        "stripe": {
          "pkg": "stripe",
          "dir": "stripe"
        },
        "paypalrestsdk": {
          "pkg": "paypalrestsdk",
          "dir": "paypalrestsdk"
        },
        "twitter": {
          "pkg": "twitter",
          "dir": "twitter"
        },
        "shippo": {
          "pkg": "shippo",
          "dir": "shippo"
        },
        "bottlenose": {
          "pkg": "bottlenose",
          "dir": "bottlenose"
        },
        "beautifulsoup4": {
          "pkg": "beautifulsoup4",
          "dir": "bs4"
        },
        "google_cloud_translate": {
          "pkg": "google-cloud-translate",
          "dir": "google/cloud/translate.py"
        }
      }
    },
    "repository": {
      "repositories": [
        {
          "owner":"anybirth",
          "name":"enybank_web"
        }
      ]
    }
  },
  "override_attributes": {
  },
  "run_list": [
    "recipe[yum]",
    "recipe[epel]",
    "recipe[vbox]",
    "recipe[vim]",
    "recipe[mysql]",
    "recipe[git]",
    "recipe[pyenv]",
    "recipe[miniconda]",
    "recipe[django]",
    "recipe[repository]"
  ]
}
